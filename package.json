{
  "name": "zyroapi",
  "version": "0.0.1-preview",
  "description": "A modern, minimalist, and fast Node.js framework for REST APIs",
  "main": "./lib/zyroapi.js",
  "types": "./index.d.ts",
  "files": [
    "lib/",
    "docs/",
    "README.md",
    "LICENSE",
    "CHANGELOG.md",
    "index.d.ts"
  ],
  "exports": {
    ".": "./lib/zyroapi.js",
    "./plugins": "./lib/plugins/index.js",
    "./plugins/json": "./lib/plugins/jsonParser.js",
    "./plugins/cors": "./lib/plugins/cors.js",
    "./plugins/base": "./lib/plugins/basePlugin.js"
  },
  "scripts": {
    "start": "node example/server.js",
    "test": "node test/test-suite.js",
    "test:verbose": "NODE_ENV=development node test/test-suite.js",
